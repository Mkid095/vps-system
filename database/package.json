{
  "name": "@nextmavenspacks/audit-logs-database",
  "version": "1.0.2",
  "description": "Database schema, migrations, and integration layer for audit logs feature",
  "type": "module",
  "main": "./dist/src/index.js",
  "types": "./dist/src/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/src/index.d.ts",
      "import": "./dist/src/index.js"
    }
  },
  "files": [
    "dist",
    "migrations",
    "types"
  ],
  "scripts": {
    "migrate": "tsx migrate.ts up",
    "migrate:status": "tsx migrate.ts status",
    "typecheck": "tsc --noEmit",
    "build": "tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:integration": "tsx src/test-integration.ts",
    "test:jobs": "vitest run src/__tests__/jobs.integration.test.ts",
    "test:queue": "vitest run src/__tests__/queue.test.ts",
    "test:admin-sessions": "vitest run src/__tests__/admin-sessions.integration.test.ts",
    "test:admin-actions": "vitest run src/__tests__/admin-actions.integration.test.ts"
  },
  "keywords": [
    "audit-logs",
    "database",
    "migrations",
    "postgresql",
    "integration",
    "jobs",
    "background-jobs",
    "task-queue",
    "worker",
    "job-queue"
  ],
  "dependencies": {
    "pg": "^8.11.3",
    "uuid": "^13.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/pg": "^8.10.9",
    "@types/uuid": "^11.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3",
    "vitest": "^4.0.18"
  }
}
