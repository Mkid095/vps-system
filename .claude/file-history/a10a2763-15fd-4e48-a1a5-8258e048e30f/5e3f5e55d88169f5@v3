#!/bin/bash
# ============================================================================
# Maven Flow - ASCII Banner
# ============================================================================

# Display ASCII art banner
show_flow_banner() {
    cat << 'EOF'

 __  __    __  _  _  ____  _  _  ___    ____  __    _____  _    _
(  \/  )  /__\( \/ )( ___)( \( )/ __)  ( ___)(  )  (  _  )(  \/\/ )
 )    (  /(__)\\  /  )__)  )  ( \__ \   )__)  )(__  )(_)(  )    (
(_/\/\_)(__)(__)\/  (____)(_)\_)(___/  (__)  (____)(_____)(__/\__)

EOF
}
